{
  "apiVersion" : "1.1.0",
  "swaggerVersion" : "1.2",
  "basePath" : "/atlas/rest",
  "resourcePath" : "/asset",
  "apis" : [ {
    "path" : "/asset/",
    "operations" : [ {
      "method" : "GET",
      "summary" : "Retrieve asset list",
      "notes" : "Retrieves a list of assets from the server. The List contains URI to the assets. The List comes in json format.",
      "type" : "array",
      "items" : {
        "type" : "string"
      },
      "nickname" : "retrieveAssetList",
      "produces" : [ "application/json" ],
      "parameters" : [ ],
      "responseMessages" : [ {
        "code" : 500,
        "message" : "Internal Server Error"
      } ]
    } ]
  }, {
    "path" : "/asset/{assetName}",
    "operations" : [ {
      "method" : "PUT",
      "summary" : "Upload asset to storage.",
      "notes" : "The asset should be within a zip file. If you choose a new asset name a new asset is created. If you choose an existing a new revision of the existing asset is added. The Response contains the URI to the transactions associated with the uploaded asset.",
      "type" : "string",
      "nickname" : "uploadZipAssetToScratchStorage",
      "consumes" : [ "application/zip", "application/x-zip-compressed" ],
      "parameters" : [ {
        "name" : "body",
        "description" : "Asset as a zip file",
        "required" : true,
        "type" : "java.io.InputStream",
        "paramType" : "body",
        "allowMultiple" : false,
        "enum" : [ "A zip file" ]
      }, {
        "name" : "assetName",
        "description" : "Asset name",
        "required" : true,
        "type" : "string",
        "paramType" : "path",
        "allowMultiple" : false,
        "enum" : [ "([A-Za-z][A-Za-z0-9_-]*)" ]
      } ],
      "responseMessages" : [ {
        "code" : 500,
        "message" : "Internal Server Error."
      } ]
    }, {
      "method" : "PUT",
      "summary" : "Upload asset from collada file to storage.",
      "notes" : " If you choose a new asset name a new asset is created. If you choose an existing a new revision of the existing asset is added. The Response contains the URI to the transactions associated with the uploaded asset.",
      "type" : "string",
      "nickname" : "uploadColladaToScratchStorage",
      "consumes" : [ "model/vnd.collada+x" ],
      "parameters" : [ {
        "name" : "body",
        "description" : "Asset as a collada file",
        "required" : true,
        "type" : "java.io.InputStream",
        "paramType" : "body",
        "allowMultiple" : false,
        "enum" : [ "A collada file" ]
      }, {
        "name" : "assetName",
        "description" : "Asset name",
        "required" : true,
        "type" : "string",
        "paramType" : "path",
        "allowMultiple" : false,
        "enum" : [ "([A-Za-z][A-Za-z0-9_-]*)" ]
      } ],
      "responseMessages" : [ {
        "code" : 500,
        "message" : "Internal Server Error."
      } ]
    } ]
  }, {
    "path" : "/asset/{assetName : ([A-Za-z][A-Za-z0-9_-]*)}{subassetPath: (/[^.]+?)?}{extension : (\\.\\w+)?}",
    "operations" : [ {
      "method" : "GET",
      "summary" : "Get encoded asset by asset name and (optional) path.",
      "notes" : "Subassets are adressed via its assetpath.",
      "type" : "void",
      "nickname" : "getEncodedAsset",
      "parameters" : [ {
        "name" : "assetName",
        "description" : "Asset name",
        "required" : true,
        "type" : "string",
        "paramType" : "path",
        "allowMultiple" : false,
        "enum" : [ "([A-Za-z][A-Za-z0-9_-]*)" ]
      }, {
        "name" : "extension",
        "description" : "File extension",
        "required" : true,
        "type" : "string",
        "paramType" : "path",
        "allowMultiple" : false,
        "enum" : [ "(\\.\\w+)?" ]
      }, {
        "name" : "subassetPath",
        "description" : "Path to the subasset",
        "required" : true,
        "type" : "string",
        "paramType" : "path",
        "allowMultiple" : false,
        "enum" : [ "(/[^.]+?)?" ]
      }, {
        "name" : "includeChildren",
        "description" : "Include child assets",
        "defaultValue" : "true",
        "required" : false,
        "type" : "boolean",
        "paramType" : "query",
        "allowMultiple" : false,
        "enum" : [ "true or false" ]
      } ],
      "responseMessages" : [ {
        "code" : 404,
        "message" : "Entity not found"
      }, {
        "code" : 406,
        "message" : "Content type is not acceptable"
      }, {
        "code" : 500,
        "message" : "Internal Server Error"
      } ]
    } ]
  } ]
}
