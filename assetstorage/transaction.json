        "name" : "id",
        "description" : "Transaction id",
        "required" : true,
        "type" : "string",
        "paramType" : "path",
        "allowMultiple" : false
      } ],
      "responseMessages" : [ {
        "code" : 404,
        "message" : "Transaction not found"
      }, {
        "code" : 500,
        "message" : "Internal Server Error"
      } ]
    } ]
  } ],
  "models" : {
