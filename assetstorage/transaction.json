        },
        "status" : {
          "$ref" : "Status",
          "description" : "Status of the transaction",
          "enum" : [ "RECEIVED", "ACCEPTED", "REJECTED", "PROCESSING", "FINISHED" ]
        },
        "assetName" : {
          "type" : "string",
          "description" : "Asset name"
        },
        "importedAs" : {
          "type" : "string",
          "description" : "Imported data type"
        },
        "statusDescription" : {
          "type" : "string",
